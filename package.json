  "license": "ISC",
  "devDependencies": {
    "cypress": "^10.10.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-multi-reporters": "^1.6.2",
    "cypress-wait-until": "^1.7.2",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0"
  },
  "dependencies": {
    "exceljs": "^4.3.0",
    "xlsx": "^0.18.5"
  }
}
